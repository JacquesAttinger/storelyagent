      '  </main>\n' +
      "  {% include 'footer' %}\n" +
      `  <script src="{{ 'app.js' | asset_url }}"></script>\n` +
      '  <script>\n' +
      '    // Enhanced search functionality with smooth animations\n' +
      "    const searchInput = document.getElementById('searchInput');\n" +
      "    const productsGrid = document.getElementById('productsGrid');\n" +
      '    if (searchInput && productsGrid) {\n' +
      "      searchInput.addEventListener('input', function(e) {\n" +
      '        const searchTerm = e.target.value.toLowerCase();\n' +
      "        const productCards = productsGrid.querySelectorAll('[data-product-card]');\n" +
      '        productCards.forEach((card, index) => {\n' +
      "          const title = card.querySelector('[data-product-title]')?.textContent.toLowerCase() || '';\n" +
      '          if (title.includes(searchTerm)) {\n' +
      "            card.style.display = '';\n" +
      "            card.style.animation = 'fadeIn 0.3s ease-out';\n" +
      "            card.style.animationDelay = (index * 0.05) + 's';\n" +
      '          } else {\n' +
      "            card.style.opacity = '0';\n" +
      "            card.style.transform = 'scale(0.9)';\n" +
      '            setTimeout(() => {\n' +
      "              card.style.display = 'none';\n" +
      '            }, 300);\n' +
      '          }\n' +
      '        });\n' +
      '      });\n' +
      '    }\n' +
      '  </script>\n' +
      '</body>\n' +
      '</html>',
