      "              {% include 'product-card', product: product %}\n" +
      '            {% endfor %}\n' +
      '          </div>\n' +
      '        {% else %}\n' +
      '          <div class="text-center py-20">\n' +
      '            <div class="inline-flex items-center justify-center w-20 h-20 mb-6 rounded-full bg-slate-800">\n' +
      '              <svg class="w-10 h-10 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
