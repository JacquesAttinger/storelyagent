    fileContents: '-- Seed Stores\n' +
      'INSERT INTO stores (id, slug, status, theme_config) VALUES\n' +
      `('store_01', 'demo-store', 'active', '{"primaryColor":"#0EA5E9","secondaryColor":"#8B5CF6","backgroundColor":"#1E293B","layoutVariant":"modern","homepageHero":{"title":"Level Up Your Gaming","subtitle":"Discover the latest games across all platforms","ctaLabel":"Browse Games"}}');\n` +
      '-- Seed Video Game Products\n' +
      'INSERT INTO products (id, store_id, title, description, price_cents, currency, image_url, stock) VALUES\n' +
      "('prod_01', 'store_01', 'Cyberpunk 2077', 'An open-world action-adventure RPG set in the dystopian Night City. Experience a branching storyline with deep character customization and immersive gameplay.', 5999, 'usd', 'https://images.unsplash.com/photo-1538481199705-c710c4e965fc?w=800&q=80', 45),\n" +
      "('prod_02', 'store_01', 'The Last of Us Part II', 'A gripping post-apocalyptic action-adventure game with stunning visuals and emotional storytelling. Navigate through a dangerous world filled with infected and hostile survivors.', 5999, 'usd', 'https://images.unsplash.com/photo-1552820728-8b83bb6b773f?w=800&q=80', 32),\n" +
      "('prod_03', 'store_01', 'Elden Ring', 'From the creators of Dark Souls comes an epic fantasy action RPG. Explore a vast open world filled with danger, mystery, and breathtaking landscapes.', 5999, 'usd', 'https://images.unsplash.com/photo-1511512578047-dfb367046420?w=800&q=80', 28),\n" +
      "('prod_04', 'store_01', 'God of War Ragnar√∂k', 'Join Kratos and Atreus on an epic journey through the Nine Realms. Experience visceral combat and a deeply personal story in Norse mythology.', 6999, 'usd', 'https://images.unsplash.com/photo-1542751371-adc38448a05e?w=800&q=80', 38),\n" +
      "('prod_05', 'store_01', 'Hogwarts Legacy', 'Experience life as a student at Hogwarts School of Witchcraft and Wizardry. Explore an open-world filled with magic, mystery, and adventure.', 5999, 'usd', 'https://images.unsplash.com/photo-1578632767115-351597cf2477?w=800&q=80', 52),\n" +
      "('prod_06', 'store_01', 'Starfield', 'Bethesda\\'s first new universe in 25 years. Explore the vast expanse of space in this next-generation role-playing game set amongst the stars.', 6999, 'usd', 'https://images.unsplash.com/photo-1614732414444-096e5f1122d5?w=800&q=80', 41),\n" +
      "('prod_07', 'store_01', 'Spider-Man 2', 'Swing through Marvel\\'s New York as both Peter Parker and Miles Morales. Face new villains and experience an original story in this action-packed adventure.', 6999, 'usd', 'https://images.unsplash.com/photo-1509198397868-475647b2a1e5?w=800&q=80', 35),\n" +
      "('prod_08', 'store_01', 'Baldur\\'s Gate 3', 'Gather your party and return to the Forgotten Realms in a tale of fellowship, betrayal, sacrifice, and survival. Based on Dungeons & Dragons 5th Edition.', 5999, 'usd', 'https://images.unsplash.com/photo-1550745165-9bc0b252726f?w=800&q=80', 47),\n" +
      "('prod_09', 'store_01', 'Zelda: Tears of the Kingdom', 'An epic adventure across the land and skies of Hyrule awaits. Harness the power of Link\\'s new abilities to fight back against malevolent forces.', 6999, 'usd', 'https://images.unsplash.com/photo-1493711662062-fa541adb3fc8?w=800&q=80', 29),\n" +
      "('prod_10', 'store_01', 'Final Fantasy XVI', 'An epic dark fantasy tale where the fate of the land is decided by the Eikons and the Dominants who wield them. Experience fast-paced action combat.', 6999, 'usd', 'https://images.unsplash.com/photo-1552820728-8b83bb6b773f?w=800&q=80', 33),\n" +
      "('prod_11', 'store_01', 'Resident Evil 4 Remake', 'Survival is just the beginning. Experience the reimagined classic with modernized gameplay, stunning visuals, and relentless horror.', 5999, 'usd', 'https://images.unsplash.com/photo-1580327344181-c1163234e5a0?w=800&q=80', 44),\n" +
      "('prod_12', 'store_01', 'Street Fighter 6', 'The evolution of fighting games starts here. Master the Drive System, explore World Tour mode, and compete in the ultimate fighting experience.', 5999, 'usd', 'https://images.unsplash.com/photo-1511512578047-dfb367046420?w=800&q=80', 39);\n" +
      '-- Seed Users\n' +
      'INSERT INTO users (id, email, role, store_id) VALUES\n' +
      "('user_01', 'admin@platform.com', 'platform_admin', NULL),\n" +
      "('user_02', 'admin@techshop.com', 'store_admin', 'store_01');",
