      '<html lang="en">\n' +
      '<head>\n' +
      '  <meta charset="UTF-8">\n' +
      '  <meta name="viewport" content="width=device-width, initial-scale=1.0">\n' +
      '  <title>{{ store.name }} - Level Up Your Gaming</title>\n' +
      `  <link rel="stylesheet" href="{{ 'index.css' | asset_url }}">\n` +
      '</head>\n' +
      '<body class="bg-slate-900 text-gray-100">\n' +
      "  {% include 'header' %}\n" +
      '  <main>\n' +
      '    <!-- Hero Section with Parallax -->\n' +
      '    <section class="relative min-h-[600px] md:min-h-[700px] flex items-center justify-center overflow-hidden" data-parallax-hero>\n' +
