    fileContents: '// TechShop - Enhanced Gaming Store JavaScript with Smooth Animations\n' +
      "document.addEventListener('DOMContentLoaded', function() {\n" +
      '  // Mobile Menu Toggle with Animation\n' +
      "  const mobileMenuBtn = document.getElementById('mobileMenuBtn');\n" +
      "  const mobileMenu = document.getElementById('mobileMenu');\n" +
      '  if (mobileMenuBtn && mobileMenu) {\n' +
      "    mobileMenuBtn.addEventListener('click', function() {\n" +
      "      const isHidden = mobileMenu.classList.contains('hidden');\n" +
      '      if (isHidden) {\n' +
      "        mobileMenu.classList.remove('hidden');\n" +
      '        setTimeout(() => {\n' +
      "          mobileMenu.classList.add('animate-slide-in-right');\n" +
      '        }, 10);\n' +
      '      } else {\n' +
      "        mobileMenu.classList.remove('animate-slide-in-right');\n" +
      '        setTimeout(() => {\n' +
      "          mobileMenu.classList.add('hidden');\n" +
      '        }, 300);\n' +
      '      }\n' +
      '      // Animate menu icon\n' +
      "      const icon = mobileMenuBtn.querySelector('svg');\n" +
      '      if (icon) {\n' +
      "        icon.classList.toggle('rotate-90');\n" +
      '      }\n' +
      '    });\n' +
      '    // Close mobile menu when clicking outside\n' +
      "    document.addEventListener('click', function(event) {\n" +
      '      if (!mobileMenuBtn.contains(event.target) && !mobileMenu.contains(event.target)) {\n' +
      "        if (!mobileMenu.classList.contains('hidden')) {\n" +
      "          mobileMenu.classList.remove('animate-slide-in-right');\n" +
      '          setTimeout(() => {\n' +
      "            mobileMenu.classList.add('hidden');\n" +
      '          }, 300);\n' +
      '        }\n' +
      '      }\n' +
      '    });\n' +
      '  }\n' +
      '  // Update cart badge with animation\n' +
      '  updateCartBadge();\n' +
      "  window.addEventListener('cartUpdated', updateCartBadge);\n" +
      '  // Smooth scroll for anchor links\n' +
      `  document.querySelectorAll('a[href^="#"]').forEach(anchor => {\n` +
      "    anchor.addEventListener('click', function(e) {\n" +
      "      const href = this.getAttribute('href');\n" +
      "      if (href !== '#' && href !== '#!') {\n" +
      '        e.preventDefault();\n' +
      '        const target = document.querySelector(href);\n' +
      '        if (target) {\n' +
      '          target.scrollIntoView({\n' +
      "            behavior: 'smooth',\n" +
      "            block: 'start'\n" +
      '          });\n' +
      '        }\n' +
      '      }\n' +
      '    });\n' +
      '  });\n' +
      '  // Intersection Observer for scroll animations\n' +
      '  const observerOptions = {\n' +
      '    threshold: 0.1,\n' +
      "    rootMargin: '0px 0px -50px 0px'\n" +
      '  };\n' +
      '  const observer = new IntersectionObserver(function(entries) {\n' +
      '    entries.forEach(entry => {\n' +
      '      if (entry.isIntersecting) {\n' +
      "        entry.target.classList.add('animate-fade-in');\n" +
      '        observer.unobserve(entry.target);\n' +
      '      }\n' +
      '    });\n' +
      '  }, observerOptions);\n' +
      '  // Observe product cards and sections\n' +
      "  document.querySelectorAll('[data-product-card], section').forEach(el => {\n" +
      '    observer.observe(el);\n' +
      '  });\n' +
      '  // Parallax effect on hero section\n' +
      "  const heroSection = document.querySelector('[data-parallax-hero]');\n" +
      '  if (heroSection) {\n' +
      "    window.addEventListener('scroll', function() {\n" +
      '      const scrolled = window.pageYOffset;\n' +
      "      const parallaxLayers = heroSection.querySelectorAll('[data-parallax-layer]');\n" +
      '      parallaxLayers.forEach(layer => {\n' +
      '        const speed = layer.dataset.parallaxSpeed || 0.5;\n' +
      '        const yPos = -(scrolled * speed);\n' +
      '        layer.style.transform = `translateY(${yPos}px)`;\n' +
      '      });\n' +
      '    });\n' +
      '  }\n' +
      '  // Product card tilt effect\n' +
      "  document.querySelectorAll('[data-tilt-card]').forEach(card => {\n" +
      "    card.addEventListener('mousemove', function(e) {\n" +
      '      const rect = card.getBoundingClientRect();\n' +
      '      const x = e.clientX - rect.left;\n' +
      '      const y = e.clientY - rect.top;\n' +
      '      const centerX = rect.width / 2;\n' +
      '      const centerY = rect.height / 2;\n' +
      '      const rotateX = (y - centerY) / 20;\n' +
      '      const rotateY = (centerX - x) / 20;\n' +
      '      card.style.transform = `perspective(1000px) rotateX(${rotateX}deg) rotateY(${rotateY}deg) scale(1.05)`;\n' +
      '    });\n' +
      "    card.addEventListener('mouseleave', function() {\n" +
      "      card.style.transform = 'perspective(1000px) rotateX(0) rotateY(0) scale(1)';\n" +
      '    });\n' +
      '  });\n' +
      '  // Add to cart button feedback with ripple effect\n' +
      "  document.addEventListener('click', function(e) {\n" +
