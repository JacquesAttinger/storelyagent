    fileContents: '// TechShop - Enhanced Cart API with Optimistic Updates and Animations\n' +
      'const CartAPI = (function() {\n' +
      "  const STORAGE_KEY = 'techshop_cart';\n" +
      "  const API_BASE = '/v1/stores/demo-store';\n" +
      '  // Get cart from localStorage\n' +
      '  function getCart() {\n' +
      '    try {\n' +
      '      const cartData = localStorage.getItem(STORAGE_KEY);\n' +
      '      return cartData ? JSON.parse(cartData) : { items: [] };\n' +
      '    } catch (err) {\n' +
      "      console.error('Failed to get cart:', err);\n" +
      '      return { items: [] };\n' +
      '    }\n' +
      '  }\n' +
      '  // Save cart to localStorage with debouncing\n' +
      '  let saveTimeout;\n' +
      '  function saveCart(cart) {\n' +
      '    clearTimeout(saveTimeout);\n' +
      '    saveTimeout = setTimeout(() => {\n' +
      '      try {\n' +
      '        localStorage.setItem(STORAGE_KEY, JSON.stringify(cart));\n' +
      "        window.dispatchEvent(new Event('cartUpdated'));\n" +
      '      } catch (err) {\n' +
      "        console.error('Failed to save cart:', err);\n" +
      '      }\n' +
      '    }, 100);\n' +
      '  }\n' +
      '  // Fetch product details\n' +
      '  async function fetchProduct(productId) {\n' +
      '    try {\n' +
      '      const response = await fetch(`${API_BASE}/products/${productId}`);\n' +
      "      if (!response.ok) throw new Error('Product not found');\n" +
      '      return await response.json();\n' +
      '    } catch (err) {\n' +
      "      console.error('Failed to fetch product:', err);\n" +
      '      return null;\n' +
      '    }\n' +
      '  }\n' +
      '  // Add item to cart with optimistic update\n' +
      '  async function addItem(productId, quantity = 1) {\n' +
      '    const cart = getCart();\n' +
      '    const existingItem = cart.items.find(item => item.productId === productId);\n' +
      '    if (existingItem) {\n' +
      '      existingItem.quantity += quantity;\n' +
      '    } else {\n' +
      '      // Optimistic update - add placeholder\n' +
      '      cart.items.push({\n' +
      '        productId,\n' +
      '        quantity,\n' +
      "        title: 'Loading...',\n" +
      '        priceCents: 0,\n' +
      '        imageUrl: null\n' +
      '      });\n' +
      '      saveCart(cart);\n' +
      '      // Fetch actual product details\n' +
      '      const product = await fetchProduct(productId);\n' +
      '      if (product) {\n' +
      '        const item = cart.items.find(i => i.productId === productId);\n' +
      '        if (item) {\n' +
      '          item.title = product.title;\n' +
      '          item.priceCents = product.priceCents;\n' +
      '          item.imageUrl = product.imageUrl;\n' +
      '          item.currency = product.currency;\n' +
      '        }\n' +
      '      } else {\n' +
      '        // Rollback on error\n' +
      '        cart.items = cart.items.filter(i => i.productId !== productId);\n' +
      "        if (typeof showToast === 'function') showToast('Failed to add item to cart', 'error');\n" +
      '      }\n' +
      '    }\n' +
      '    saveCart(cart);\n' +
      '    return cart;\n' +
      '  }\n' +
      '  // Update item quantity with validation\n' +
      '  function updateQuantity(productId, quantity) {\n' +
      '    const cart = getCart();\n' +
      '    const item = cart.items.find(i => i.productId === productId);\n' +
      '    if (!item) return cart;\n' +
      '    if (quantity <= 0) {\n' +
      '      return removeItem(productId);\n' +
      '    }\n' +
      '    // Validate against stock if available\n' +
      '    if (item.stock && quantity > item.stock) {\n' +
      "      if (typeof showToast === 'function') showToast(`Only ${item.stock} items available`, 'error');\n" +
      '      return cart;\n' +
      '    }\n' +
      '    item.quantity = quantity;\n' +
      '    saveCart(cart);\n' +
      '    return cart;\n' +
      '  }\n' +
      '  // Remove item from cart with animation\n' +
      '  function removeItem(productId) {\n' +
      '    const cart = getCart();\n' +
      '    cart.items = cart.items.filter(item => item.productId !== productId);\n' +
      '    saveCart(cart);\n' +
      "    if (typeof showToast === 'function') showToast('Item removed from cart', 'info');\n" +
      '    return cart;\n' +
      '  }\n' +
      '  // Clear entire cart\n' +
      '  function clearCart() {\n' +
      '    const cart = { items: [] };\n' +
      '    saveCart(cart);\n' +
      '    return cart;\n' +
      '  }\n' +
      '  // Get cart item count\n' +
      '  function getItemCount() {\n' +
      '    const cart = getCart();\n' +
      '    return cart.items.reduce((sum, item) => sum + item.quantity, 0);\n' +
      '  }\n' +
      '  // Get cart total\n' +
      '  function getTotal() {\n' +
      '    const cart = getCart();\n' +
      '    return cart.items.reduce((sum, item) => sum + (item.priceCents * item.quantity), 0);\n' +
      '  }\n' +
      '  // Checkout process\n' +
      '  async function checkout() {\n' +
      '    const cart = getCart();\n' +
      '    if (!cart.items || cart.items.length === 0) {\n' +
      "      if (typeof showToast === 'function') showToast('Your cart is empty', 'error');\n" +
      '      return;\n' +
      '    }\n' +
      '    try {\n' +
      '      const response = await fetch(`${API_BASE}/checkout`, {\n' +
      "        method: 'POST',\n" +
      '        headers: {\n' +
      "          'Content-Type': 'application/json'\n" +
      '        },\n' +
      '        body: JSON.stringify({\n' +
      '          items: cart.items.map(item => ({\n' +
      '            productId: item.productId,\n' +
      '            quantity: item.quantity\n' +
      '          })),\n' +
      "          email: 'customer@example.com'\n" +
      '        })\n' +
      '      });\n' +
      '      if (!response.ok) {\n' +
      "        throw new Error('Checkout failed');\n" +
      '      }\n' +
      '      const data = await response.json();\n' +
      '      if (data.checkoutUrl) {\n' +
      '        window.location.href = data.checkoutUrl;\n' +
      '      } else {\n' +
      "        throw new Error('No checkout URL received');\n" +
      '      }\n' +
      '    } catch (err) {\n' +
      "      console.error('Checkout error:', err);\n" +
      "      if (typeof showToast === 'function') showToast('Checkout failed. Please try again.', 'error');\n" +
      '    }\n' +
      '  }\n' +
      '  // Toast notification helper\n' +
      "  function showToast(message, type = 'info') {\n" +
      "    if (typeof window.showToast === 'function') {\n" +
      '      window.showToast(message, type);\n' +
      '    } else {\n' +
      '      console.log(`[${type.toUpperCase()}] ${message}`);\n' +
      '    }\n' +
      '  }\n' +
      '  // Public API\n' +
      '  return {\n' +
      '    getCart,\n' +
      '    addItem,\n' +
      '    updateQuantity,\n' +
      '    removeItem,\n' +
      '    clearCart,\n' +
      '    getItemCount,\n' +
      '    getTotal,\n' +
      '    checkout\n' +
      '  };\n' +
      '})();\n' +
      '// Make CartAPI globally available\n' +
      'window.CartAPI = CartAPI;',
