      '          >\n' +
      '            View All Games\n' +
      '            <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
      '              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>\n' +
      '            </svg>\n' +
      '          </a>\n' +
      '        </div>\n' +
      '      </div>\n' +
      '    </section>\n' +
      '    <!-- Why Choose Us Section -->\n' +
      '    <section class="py-16 md:py-20 bg-slate-800/50">\n' +
      '      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">\n' +
      '        <div class="grid grid-cols-1 md:grid-cols-3 gap-8 md:gap-12">\n' +
      '          <div class="text-center transform hover:scale-105 transition-transform duration-300">\n' +
      '            <div class="inline-flex items-center justify-center w-16 h-16 mb-4 rounded-full bg-gradient-to-br from-sky-500 to-blue-600 shadow-lg animate-float">\n' +
      '              <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
      '                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>\n' +
      '              </svg>\n' +
      '            </div>\n' +
      '            <h3 class="text-xl font-bold mb-2 text-gray-100">Instant Delivery</h3>\n' +
      '            <p class="text-gray-400">Get your games instantly after purchase. No waiting, just gaming.</p>\n' +
      '          </div>\n' +
      '          <div class="text-center transform hover:scale-105 transition-transform duration-300">\n' +
      '            <div class="inline-flex items-center justify-center w-16 h-16 mb-4 rounded-full bg-gradient-to-br from-purple-500 to-purple-600 shadow-lg animate-float" style="animation-delay: 0.2s;">\n' +
      '              <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
      '                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"></path>\n' +
      '              </svg>\n' +
      '            </div>\n' +
      '            <h3 class="text-xl font-bold mb-2 text-gray-100">Secure Payments</h3>\n' +
      '            <p class="text-gray-400">Shop with confidence using our secure payment system.</p>\n' +
      '          </div>\n' +
      '          <div class="text-center transform hover:scale-105 transition-transform duration-300">\n' +
      '            <div class="inline-flex items-center justify-center w-16 h-16 mb-4 rounded-full bg-gradient-to-br from-sky-500 to-purple-600 shadow-lg animate-float" style="animation-delay: 0.4s;">\n' +
      '              <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
      '                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 10h4.764a2 2 0 011.789 2.894l-3.5 7A2 2 0 0115.263 21h-4.017c-.163 0-.326-.02-.485-.06L7 20m7-10V5a2 2 0 00-2-2h-.095c-.5 0-.905.405-.905.905 0 .714-.211 1.412-.608 2.006L7 11v9m7-10h-2M7 20H5a2 2 0 01-2-2v-6a2 2 0 012-2h2.5"></path>\n' +
      '              </svg>\n' +
      '            </div>\n' +
      '            <h3 class="text-xl font-bold mb-2 text-gray-100">Best Selection</h3>\n' +
      '            <p class="text-gray-400">Curated collection of the best games across all platforms.</p>\n' +
      '          </div>\n' +
      '        </div>\n' +
      '      </div>\n' +
      '    </section>\n' +
      '  </main>\n' +
      "  {% include 'footer' %}\n" +
      `  <script src="{{ 'app.js' | asset_url }}"></script>\n` +
      '</body>\n' +
      '</html>',
