      '// Mobile Menu Toggle\n' +
      "document.addEventListener('DOMContentLoaded', function() {\n" +
      "  const mobileMenuBtn = document.getElementById('mobileMenuBtn');\n" +
      "  const mobileMenu = document.getElementById('mobileMenu');\n" +
      '  if (mobileMenuBtn && mobileMenu) {\n' +
      "    mobileMenuBtn.addEventListener('click', function() {\n" +
      "      mobileMenu.classList.toggle('hidden');\n" +
      '      // Animate menu icon\n' +
      "      const icon = mobileMenuBtn.querySelector('svg');\n" +
      '      if (icon) {\n' +
      "        icon.classList.toggle('rotate-90');\n" +
      '      }\n' +
      '    });\n' +
      '    // Close mobile menu when clicking outside\n' +
      "    document.addEventListener('click', function(event) {\n" +
      '      if (!mobileMenuBtn.contains(event.target) && !mobileMenu.contains(event.target)) {\n' +
      "        mobileMenu.classList.add('hidden');\n" +
      '      }\n' +
      '    });\n' +
      '  }\n' +
      '  // Update cart badge\n' +
      '  updateCartBadge();\n' +
      '  // Listen for cart updates\n' +
      "  window.addEventListener('cartUpdated', updateCartBadge);\n" +
      '  // Smooth scroll for anchor links\n' +
      `  document.querySelectorAll('a[href^="#"]').forEach(anchor => {\n` +
      "    anchor.addEventListener('click', function(e) {\n" +
      "      const href = this.getAttribute('href');\n" +
      "      if (href !== '#' && href !== '#!') {\n" +
      '        e.preventDefault();\n' +
      '        const target = document.querySelector(href);\n' +
      '        if (target) {\n' +
      '          target.scrollIntoView({\n' +
      "            behavior: 'smooth',\n" +
      "            block: 'start'\n" +
      '          });\n' +
      '        }\n' +
      '      }\n' +
      '    });\n' +
      '  });\n' +
      '  // Add scroll animations\n' +
      '  const observerOptions = {\n' +
      '    threshold: 0.1,\n' +
      "    rootMargin: '0px 0px -50px 0px'\n" +
      '  };\n' +
      '  const observer = new IntersectionObserver(function(entries) {\n' +
      '    entries.forEach(entry => {\n' +
      '      if (entry.isIntersecting) {\n' +
      "        entry.target.classList.add('animate-fade-in');\n" +
      '        observer.unobserve(entry.target);\n' +
      '      }\n' +
      '    });\n' +
      '  }, observerOptions);\n' +
      '  // Observe product cards and sections\n' +
      "  document.querySelectorAll('[data-product-card], section').forEach(el => {\n" +
      '    observer.observe(el);\n' +
      '  });\n' +
      '  // Add to cart button feedback\n' +
      "  document.addEventListener('click', function(e) {\n" +
      `    const addToCartBtn = e.target.closest('[onclick*="CartAPI.addItem"]');\n` +
      '    if (addToCartBtn) {\n' +
      '      // Visual feedback\n' +
      "      addToCartBtn.classList.add('scale-95');\n" +
      '      setTimeout(() => {\n' +
      "        addToCartBtn.classList.remove('scale-95');\n" +
      '      }, 150);\n' +
      '      // Show toast notification\n' +
      "      showToast('Added to cart!', 'success');\n" +
      '    }\n' +
      '  });\n' +
      '  // Platform filter functionality (if on products page)\n' +
      "  const platformButtons = document.querySelectorAll('[data-platform-filter]');\n" +
      '  platformButtons.forEach(button => {\n' +
      "    button.addEventListener('click', function() {\n" +
      '      const platform = this.dataset.platformFilter;\n' +
      '      filterByPlatform(platform);\n' +
      '      // Update active state\n' +
      "      platformButtons.forEach(btn => btn.classList.remove('active'));\n" +
      "      this.classList.add('active');\n" +
      '    });\n' +
      '  });\n' +
      '});\n' +
      '// Update cart badge count\n' +
      'function updateCartBadge() {\n' +
      "  if (typeof CartAPI === 'undefined') return;\n" +
      '  const cart = CartAPI.getCart();\n' +
      "  const badge = document.getElementById('cartBadge');\n" +
      '  if (!badge) return;\n' +
      '  if (cart && cart.items.length > 0) {\n' +
      '    const totalItems = cart.items.reduce((sum, item) => sum + item.quantity, 0);\n' +
      '    badge.textContent = totalItems;\n' +
      "    badge.classList.remove('hidden');\n" +
      '    // Add pulse animation\n' +
      "    badge.classList.add('animate-pulse-glow');\n" +
      '    setTimeout(() => {\n' +
      "      badge.classList.remove('animate-pulse-glow');\n" +
      '    }, 1000);\n' +
      '  } else {\n' +
      "    badge.classList.add('hidden');\n" +
      '  }\n' +
      '}\n' +
      '// Filter products by platform\n' +
      'function filterByPlatform(platform) {\n' +
      "  const productCards = document.querySelectorAll('[data-product-card]');\n" +
      '  productCards.forEach(card => {\n' +
      "    if (platform === 'all') {\n" +
      "      card.style.display = '';\n" +
      '    } else {\n' +
      "      const platformBadge = card.querySelector('[data-platform]');\n" +
      '      if (platformBadge && platformBadge.dataset.platform === platform) {\n' +
      "        card.style.display = '';\n" +
      '      } else {\n' +
      "        card.style.display = 'none';\n" +
      '      }\n' +
      '    }\n' +
      '  });\n' +
      '}\n' +
      '// Toast notification system\n' +
      "function showToast(message, type = 'info') {\n" +
      "  const toast = document.createElement('div');\n" +
      '  toast.className = `fixed bottom-4 right-4 px-6 py-4 rounded-lg shadow-2xl z-50 animate-slide-up ${\n' +
      "    type === 'success' ? 'bg-green-500 text-white' :\n" +
      "    type === 'error' ? 'bg-red-500 text-white' :\n" +
      "    'bg-slate-800 text-gray-100 border border-slate-700'\n" +
      '  }`;\n' +
      '  toast.textContent = message;\n' +
      '  document.body.appendChild(toast);\n' +
      '  setTimeout(() => {\n' +
      "    toast.classList.add('opacity-0');\n" +
      '    setTimeout(() => {\n' +
      '      document.body.removeChild(toast);\n' +
      '    }, 300);\n' +
      '  }, 3000);\n' +
      '}\n' +
      '// Image lazy loading with fade-in effect\n' +
      "if ('IntersectionObserver' in window) {\n" +
      '  const imageObserver = new IntersectionObserver((entries, observer) => {\n' +
      '    entries.forEach(entry => {\n' +
      '      if (entry.isIntersecting) {\n' +
      '        const img = entry.target;\n' +
      '        img.src = img.dataset.src;\n' +
      "        img.classList.add('animate-fade-in');\n" +
      '        observer.unobserve(img);\n' +
      '      }\n' +
      '    });\n' +
      '  });\n' +
      "  document.querySelectorAll('img[data-src]').forEach(img => {\n" +
      '    imageObserver.observe(img);\n' +
      '  });\n' +
      '}\n' +
      '// Prevent form submission on Enter key (for search inputs)\n' +
      `document.querySelectorAll('input[type="text"]').forEach(input => {\n` +
      "  input.addEventListener('keypress', function(e) {\n" +
      "    if (e.key === 'Enter' && !this.form) {\n" +
      '      e.preventDefault();\n' +
      '    }\n' +
      '  });\n' +
      '});\n' +
      '// Add loading state to checkout button\n' +
      "document.addEventListener('click', function(e) {\n" +
      `  const checkoutBtn = e.target.closest('[onclick*="CartAPI.checkout"]');\n` +
      '  if (checkoutBtn) {\n' +
      '    checkoutBtn.disabled = true;\n' +
      '    checkoutBtn.innerHTML = `\n' +
      '      <svg class="animate-spin h-5 w-5 mx-auto" fill="none" viewBox="0 0 24 24">\n' +
      '        <circle class="opacity-25" cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle>\n' +
      '        <path class="opacity-75" fill="currentColor" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path>\n' +
      '      </svg>\n' +
      '    `;\n' +
      '  }\n' +
      '});\n' +
      '// Console welcome message\n' +
      "console.log('%cTechShop', 'font-size: 24px; font-weight: bold; background: linear-gradient(to right, #0EA5E9, #8B5CF6); -webkit-background-clip: text; -webkit-text-fill-color: transparent;');\n" +
