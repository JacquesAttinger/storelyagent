    fileContents: '// TechShop - Enhanced Gaming Store JavaScript with Smooth Animations\n' +
      "document.addEventListener('DOMContentLoaded', function() {\n" +
      '  // Mobile Menu Toggle with Animation\n' +
      "  const mobileMenuBtn = document.getElementById('mobileMenuBtn');\n" +
      "  const mobileMenu = document.getElementById('mobileMenu');\n" +
      '  if (mobileMenuBtn && mobileMenu) {\n' +
      "    mobileMenuBtn.addEventListener('click', function() {\n" +
      "      const isHidden = mobileMenu.classList.contains('hidden');\n" +
      '      if (isHidden) {\n' +
      "        mobileMenu.classList.remove('hidden');\n" +
      '        setTimeout(() => {\n' +
      "          mobileMenu.classList.add('animate-slide-in-right');\n" +
      '        }, 10);\n' +
      '      } else {\n' +
      "        mobileMenu.classList.remove('animate-slide-in-right');\n" +
      '        setTimeout(() => {\n' +
      "          mobileMenu.classList.add('hidden');\n" +
      '        }, 300);\n' +
      '      }\n' +
      '      // Animate menu icon\n' +
      "      const icon = mobileMenuBtn.querySelector('svg');\n" +
      '      if (icon) {\n' +
      "        icon.classList.toggle('rotate-90');\n" +
      '      }\n' +
      '    });\n' +
      '    // Close mobile menu when clicking outside\n' +
      "    document.addEventListener('click', function(event) {\n" +
      '      if (!mobileMenuBtn.contains(event.target) && !mobileMenu.contains(event.target)) {\n' +
      "        if (!mobileMenu.classList.contains('hidden')) {\n" +
      "          mobileMenu.classList.remove('animate-slide-in-right');\n" +
      '          setTimeout(() => {\n' +
      "            mobileMenu.classList.add('hidden');\n" +
      '          }, 300);\n' +
      '        }\n' +
      '      }\n' +
      '    });\n' +
      '  }\n' +
      '  // Update cart badge with animation\n' +
      '  updateCartBadge();\n' +
      "  window.addEventListener('cartUpdated', updateCartBadge);\n" +
      '  // Smooth scroll for anchor links\n' +
      `  document.querySelectorAll('a[href^="#"]').forEach(anchor => {\n` +
      "    anchor.addEventListener('click', function(e) {\n" +
      "      const href = this.getAttribute('href');\n" +
      "      if (href !== '#' && href !== '#!') {\n" +
      '        e.preventDefault();\n' +
      '        const target = document.querySelector(href);\n' +
      '        if (target) {\n' +
      '          target.scrollIntoView({\n' +
      "            behavior: 'smooth',\n" +
      "            block: 'start'\n" +
      '          });\n' +
      '        }\n' +
      '      }\n' +
      '    });\n' +
      '  });\n' +
      '  // Intersection Observer for scroll animations\n' +
      '  const observerOptions = {\n' +
      '    threshold: 0.1,\n' +
      "    rootMargin: '0px 0px -50px 0px'\n" +
      '  };\n' +
      '  const observer = new IntersectionObserver(function(entries) {\n' +
      '    entries.forEach(entry => {\n' +
      '      if (entry.isIntersecting) {\n' +
      "        entry.target.classList.add('animate-fade-in');\n" +
      '        observer.unobserve(entry.target);\n' +
      '      }\n' +
      '    });\n' +
      '  }, observerOptions);\n' +
      '  // Observe product cards and sections\n' +
      "  document.querySelectorAll('[data-product-card], section').forEach(el => {\n" +
      '    observer.observe(el);\n' +
      '  });\n' +
      '  // Parallax effect on hero section\n' +
      "  const heroSection = document.querySelector('[data-parallax-hero]');\n" +
      '  if (heroSection) {\n' +
      "    window.addEventListener('scroll', function() {\n" +
      '      const scrolled = window.pageYOffset;\n' +
      "      const parallaxLayers = heroSection.querySelectorAll('[data-parallax-layer]');\n" +
      '      parallaxLayers.forEach(layer => {\n' +
      '        const speed = layer.dataset.parallaxSpeed || 0.5;\n' +
      '        const yPos = -(scrolled * speed);\n' +
      '        layer.style.transform = `translateY(${yPos}px)`;\n' +
      '      });\n' +
      '    });\n' +
      '  }\n' +
      '  // Product card tilt effect\n' +
      "  document.querySelectorAll('[data-tilt-card]').forEach(card => {\n" +
      "    card.addEventListener('mousemove', function(e) {\n" +
      '      const rect = card.getBoundingClientRect();\n' +
      '      const x = e.clientX - rect.left;\n' +
      '      const y = e.clientY - rect.top;\n' +
      '      const centerX = rect.width / 2;\n' +
      '      const centerY = rect.height / 2;\n' +
      '      const rotateX = (y - centerY) / 20;\n' +
      '      const rotateY = (centerX - x) / 20;\n' +
      '      card.style.transform = `perspective(1000px) rotateX(${rotateX}deg) rotateY(${rotateY}deg) scale(1.05)`;\n' +
      '    });\n' +
      "    card.addEventListener('mouseleave', function() {\n" +
      "      card.style.transform = 'perspective(1000px) rotateX(0) rotateY(0) scale(1)';\n" +
      '    });\n' +
      '  });\n' +
      '  // Add to cart button feedback with ripple effect\n' +
      "  document.addEventListener('click', function(e) {\n" +
      `    const addToCartBtn = e.target.closest('[onclick*="CartAPI.addItem"]');\n` +
      '    if (addToCartBtn) {\n' +
      '      // Visual feedback\n' +
      "      addToCartBtn.classList.add('scale-95');\n" +
      '      setTimeout(() => {\n' +
      "        addToCartBtn.classList.remove('scale-95');\n" +
      '      }, 150);\n' +
      '      // Ripple effect\n' +
      "      const ripple = document.createElement('span');\n" +
      "      ripple.style.position = 'absolute';\n" +
      "      ripple.style.borderRadius = '50%';\n" +
      "      ripple.style.background = 'rgba(255, 255, 255, 0.5)';\n" +
      "      ripple.style.width = '20px';\n" +
      "      ripple.style.height = '20px';\n" +
      "      ripple.style.animation = 'ripple 0.6s ease-out';\n" +
      '      const rect = addToCartBtn.getBoundingClientRect();\n' +
      "      ripple.style.left = (e.clientX - rect.left - 10) + 'px';\n" +
      "      ripple.style.top = (e.clientY - rect.top - 10) + 'px';\n" +
      "      addToCartBtn.style.position = 'relative';\n" +
      "      addToCartBtn.style.overflow = 'hidden';\n" +
      '      addToCartBtn.appendChild(ripple);\n' +
      '      setTimeout(() => {\n' +
      '        ripple.remove();\n' +
      '      }, 600);\n' +
      '      // Show toast notification\n' +
      "      showToast('Added to cart!', 'success');\n" +
      '    }\n' +
      '  });\n' +
      '  // Keyboard navigation for product grids\n' +
      "  const productGrid = document.querySelector('[data-product-grid]');\n" +
      '  if (productGrid) {\n' +
      "    const productCards = Array.from(productGrid.querySelectorAll('[data-product-card]'));\n" +
      '    let currentIndex = -1;\n' +
      "    document.addEventListener('keydown', function(e) {\n" +
      '      if (!productCards.length) return;\n' +
      "      if (e.key === 'ArrowRight') {\n" +
      '        e.preventDefault();\n' +
      '        currentIndex = (currentIndex + 1) % productCards.length;\n' +
      "        productCards[currentIndex].querySelector('a').focus();\n" +
      "      } else if (e.key === 'ArrowLeft') {\n" +
      '        e.preventDefault();\n' +
      '        currentIndex = currentIndex <= 0 ? productCards.length - 1 : currentIndex - 1;\n' +
      "        productCards[currentIndex].querySelector('a').focus();\n" +
      '      }\n' +
      '    });\n' +
      '  }\n' +
      '  // Sticky header with fade-in on scroll\n' +
      "  const header = document.querySelector('header');\n" +
      '  if (header) {\n' +
      '    let lastScroll = 0;\n' +
      "    window.addEventListener('scroll', function() {\n" +
      '      const currentScroll = window.pageYOffset;\n' +
      '      if (currentScroll > 100) {\n' +
      "        header.classList.add('shadow-2xl');\n" +
      '      } else {\n' +
      "        header.classList.remove('shadow-2xl');\n" +
      '      }\n' +
      '      lastScroll = currentScroll;\n' +
      '    });\n' +
      '  }\n' +
      '  // Back to top button\n' +
      "  const backToTop = document.createElement('button');\n" +
      '  backToTop.innerHTML = `\n' +
      '    <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
      '      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 10l7-7m0 0l7 7m-7-7v18"></path>\n' +
      '    </svg>\n' +
      '  `;\n' +
      "  backToTop.className = 'fixed bottom-8 right-8 p-4 bg-gradient-to-r from-sky-500 to-blue-600 text-white rounded-full shadow-lg hover:shadow-sky-500/50 transition-all duration-300 hover:scale-110 z-50 hidden';\n" +
      "  backToTop.setAttribute('aria-label', 'Back to top');\n" +
      '  document.body.appendChild(backToTop);\n' +
      "  window.addEventListener('scroll', function() {\n" +
      '    if (window.pageYOffset > 500) {\n' +
      "      backToTop.classList.remove('hidden');\n" +
      "      backToTop.classList.add('animate-fade-in-up');\n" +
      '    } else {\n' +
      "      backToTop.classList.add('hidden');\n" +
      "      backToTop.classList.remove('animate-fade-in-up');\n" +
      '    }\n' +
      '  });\n' +
      "  backToTop.addEventListener('click', function() {\n" +
      '    window.scrollTo({\n' +
      '      top: 0,\n' +
      "      behavior: 'smooth'\n" +
      '    });\n' +
      '  });\n' +
      '  // Image lazy loading with fade-in effect\n' +
      "  if ('IntersectionObserver' in window) {\n" +
      '    const imageObserver = new IntersectionObserver((entries, observer) => {\n' +
      '      entries.forEach(entry => {\n' +
      '        if (entry.isIntersecting) {\n' +
      '          const img = entry.target;\n' +
      '          if (img.dataset.src) {\n' +
      '            img.src = img.dataset.src;\n' +
      "            img.classList.add('animate-fade-in');\n" +
      '            observer.unobserve(img);\n' +
