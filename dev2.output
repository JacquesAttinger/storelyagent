    fileContents: '<div class="group" data-product-card>\n' +
      '  <a href="/products/{{ product.id }}" class="block">\n' +
      '    <!-- Product Image -->\n' +
      '    <div class="relative aspect-[3/4] rounded-xl overflow-hidden bg-slate-800 mb-4 shadow-lg group-hover:shadow-2xl group-hover:shadow-sky-500/20 transition-all duration-300">\n' +
      '      {% if product.imageUrl %}\n' +
      '        <img \n' +
      '          src="{{ product.imageUrl }}" \n' +
      '          alt="{{ product.title }}"\n' +
      '          class="w-full h-full object-cover group-hover:scale-110 transition-transform duration-500"\n' +
      '        />\n' +
      '      {% else %}\n' +
      '        <div class="w-full h-full flex items-center justify-center">\n' +
      '          <svg class="w-16 h-16 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">\n' +
      '            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 16l4.586-4.586a2 2 0 012.828 0L16 16m-2-2l1.586-1.586a2 2 0 012.828 0L20 14m-6-6h.01M6 20h12a2 2 0 002-2V6a2 2 0 00-2-2H6a2 2 0 00-2 2v12a2 2 0 002 2z"></path>\n' +
      '          </svg>\n' +
      '        </div>\n' +
      '      {% endif %}\n' +
      '      <!-- Gradient Overlay on Hover -->\n' +
      '      <div class="absolute inset-0 bg-gradient-to-t from-slate-900 via-slate-900/40 to-transparent opacity-0 group-hover:opacity-100 transition-opacity duration-300"></div>\n' +
      '      <!-- Platform Badge -->\n' +
      '      <div class="absolute top-3 right-3 px-3 py-1 text-xs font-bold text-white bg-sky-500/90 backdrop-blur-sm rounded-full shadow-lg">\n' +
      '        PC\n' +
      '      </div>\n' +
      '      <!-- Stock Badge -->\n' +
      '      {% if product.stock %}\n' +
      '        {% if product.stock > 10 %}\n' +
      '          <div class="absolute top-3 left-3 px-3 py-1 text-xs font-medium text-green-400 bg-green-400/10 backdrop-blur-sm border border-green-400/20 rounded-full">\n' +
      '            In Stock\n' +
      '          </div>\n' +
      '        {% elsif product.stock > 0 %}\n' +
      '          <div class="absolute top-3 left-3 px-3 py-1 text-xs font-medium text-yellow-400 bg-yellow-400/10 backdrop-blur-sm border border-yellow-400/20 rounded-full">\n' +
      '            Low Stock\n' +
      '          </div>\n' +
      '        {% else %}\n' +
      '          <div class="absolute top-3 left-3 px-3 py-1 text-xs font-medium text-red-400 bg-red-400/10 backdrop-blur-sm border border-red-400/20 rounded-full">\n' +
      '            Sold Out\n' +
      '          </div>\n' +
      '        {% endif %}\n' +
      '      {% endif %}\n' +
      '      <!-- Quick View Button (appears on hover) -->\n' +
      '      <div class="absolute bottom-4 left-4 right-4 opacity-0 group-hover:opacity-100 transition-opacity duration-300">\n' +
      '        <button class="w-full px-4 py-2 text-sm font-semibold text-white bg-gradient-to-r from-sky-500 to-blue-600 rounded-lg hover:from-sky-600 hover:to-blue-700 transition-all shadow-lg">\n' +
      '          View Details\n' +
      '        </button>\n' +
      '      </div>\n' +
      '    </div>\n' +
      '    <!-- Product Info -->\n' +
